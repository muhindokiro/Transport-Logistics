<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_open_file_document_tree" model="ir.ui.view">
        <field name="name">open.file.document.tree</field>
        <field name="model">open.file.document</field>
        <field name="arch" type="xml">
            <tree string="Documents Center">
                <field name="date"/>
                <field name="name"/>
                <field name="name"/>
                <field name="document"/>
            </tree>
        </field>
    </record>

    <record id="view_open_file_document_form" model="ir.ui.view">
        <field name="name">open.file.document.form</field>
        <field name="model">open.file.document</field>
        <field name="arch" type="xml">
               <form>
                <sheet>
                    <group>
                        <field name="date"/>
                        <field name="name"/>
                        <field name="file_ref"/>
                        <field name="assoc_file"/>
                        <field name="document"/>
                    </group>
                </sheet>
               </form>
        </field>
    </record>
</odoo>