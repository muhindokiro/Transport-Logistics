<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="file_system_sequence" model="ir.sequence">
            <field name="name">File System Sequence</field>
            <field name="code">open.file</field>
            <field name="prefix">RTC/</field>
            <field name="suffix">/%(y)s</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="bond_number_sequence" model="ir.sequence">
            <field name="name">Bond Number Sequence</field>
            <field name="code">bond.number</field>
            <field name="prefix">RTCG/</field>
            <field name="suffix">/%(y)s</field>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
            <field name="company_id" eval="False" />
        </record>

    </data>
</odoo>